<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.web.blog.model.repo.RoadmapRepo">

	<select id="selectRoadmap" parameterType="int"
		resultType="Roadmap">
		select *
		from roadmap
		where rmid = #{rmid}
	</select>
	<select id="selectRoadmapListByUid" parameterType="int"
		resultType="Roadmap">
		select *
		from roadmap
		where uid = #{uid}
	</select>
	<select id="selectRoadmapListByRmorder" parameterType="int"
		resultType="Roadmap">
		select *
		from roadmap
		where rmorder = #{rmorder}
	</select>
	<insert id="insert" parameterType="Roadmap">
		insert
		into roadmap(uid, name,
		rmorder, tmp)
		values(#{uid}, #{name}, #{rmorder},#{tmp})
	</insert>
</mapper>
	